generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Person {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

model Item {
  id        String   @id @default(uuid())
  name      String
  quantity  Int
  createdAt DateTime @default(now())
}